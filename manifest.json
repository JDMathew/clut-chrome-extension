{
  "manifest_version": 2,

  "name": "Cycle Last Used Tabs",
  "description": "Cycle through last used Chrome tabs",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts" : ["background.js"]
  },
  "commands": {
    "alt_switch": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Toggle feature foo"
    }
  } 

}